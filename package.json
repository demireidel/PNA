{
  "name": "pna-full-build",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "deadcode": "knip",
    "unused:types": "ts-prune",
    "build:zip:standalone": "next build && node scripts/zip-standalone.mjs",
    "build:zip:vercel": "vercel build --prod && node scripts/zip-vercel-output.mjs",
    "build:zip:static": "next build && next export && (cd out && zip -r ../static-export.zip . >/dev/null || powershell -noprofile -command Compress-Archive -Path * -DestinationPath ../static-export.zip -Force)"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "dependencies": {
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@react-three/fiber": "9.0.0-beta.71",
    "@react-three/drei": "10.7.1",
    "three": "0.170.0"
  },
  "devDependencies": {
    "typescript": "5.6.3",
    "@types/node": "22.7.4",
    "@types/react": "19.0.1",
    "eslint": "9.9.0",
    "@typescript-eslint/parser": "8.4.0",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "@next/bundle-analyzer": "15.0.0",
    "knip": "5.33.0",
    "ts-prune": "0.10.3",
    "vercel": "39.3.0"
  }
}
